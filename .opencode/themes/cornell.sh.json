      "dark": "white",
      "light": "black"
    },
    "markdownHeading": {
      "dark": "purple",
      "light": "purple"
    },
    "markdownLink": {
      "dark": "cyan",
      "light": "cyan"
    },
    "markdownLinkText": {
      "dark": "cyanAccent",
      "light": "cyanAccent"
    },
    "markdownCode": {
      "dark": "green",
      "light": "green"
    },
    "markdownBlockQuote": {
      "dark": "gray",
      "light": "#909090"
    },
    "markdownEmph": {
      "dark": "pink",
      "light": "pink"
    },
    "markdownStrong": {
      "dark": "yellow",
      "light": "#c4a000"
    },
    "markdownHorizontalRule": {
      "dark": "gray",
      "light": "#909090"
    },
    "markdownListItem": {
      "dark": "purple",
      "light": "purple"
    },
    "markdownListEnumeration": {
      "dark": "cyanAccent",
      "light": "cyanAccent"
    },
    "markdownImage": {
      "dark": "cyan",
      "light": "cyan"
    },
    "markdownImageText": {
      "dark": "cyanAccent",
      "light": "cyanAccent"
    },
    "markdownCodeBlock": {
      "dark": "white",
      "light": "black"
    },
    "syntaxComment": {
      "dark": "gray",
      "light": "#606060"
    },
    "syntaxKeyword": {
      "dark": "purple",
      "light": "#7c3aed"
    },
    "syntaxFunction": {
      "dark": "cyan",
      "light": "#0891b2"
    },
    "syntaxVariable": {
      "dark": "pink",
      "light": "#c026d3"
    },
    "syntaxString": {
      "dark": "green",
      "light": "#15803d"
    },
    "syntaxNumber": {
      "dark": "red",
      "light": "#b91c1c"
    },
    "syntaxType": {
      "dark": "cyanAccent",
      "light": "#06b6d4"
    },
    "syntaxOperator": {
      "dark": "yellow",
      "light": "#a16207"
    },
    "syntaxPunctuation": {
      "dark": "white",
      "light": "#606060"
    }
  }
}
